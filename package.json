    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "find-root": "^1.0.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
